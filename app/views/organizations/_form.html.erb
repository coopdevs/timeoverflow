<%= show_error_messages!(@organization) %>
<%= simple_form_for @organization do |f| %>
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :phone %>
  <%= f.input :web %>
  <%= f.input :description %>
  <%= f.input :public_opening_times %>
  <%= f.input :address %>
  <%= f.input :neighborhood %>
  <%= f.input :city %>
  <%= f.button :submit, class: "mdc-button mdc-button--raised" %>
<% end %>
