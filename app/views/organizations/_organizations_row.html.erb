<tr>
  <td><%= link_to(org.name, org) %></td>
  <td><%= org.city %></td>
  <td><%= org.neighborhood %></td>
  <td><%= link_to(org.web, org.web) if org.web.present? %></td>
  <td><%= org.members.count %></td>
  <td>
    <%= render "organizations/petition_button", organization: org %>
  </td>
  <% if current_user&.manages?(current_organization) %>
    <td>
      <%= render "organizations/alliance_button", organization: org %>
    </td>
  <% end %>
</tr>